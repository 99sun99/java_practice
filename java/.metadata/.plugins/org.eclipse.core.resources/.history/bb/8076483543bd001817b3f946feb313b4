package formal;

import java.util.*;

public class Item implements Comparable {
	private String id;
	private String name;
	private double retail;
	private int quantity;
	private double price;

	Item(String idIn,String nameIn, String  retailIn, String quanIn ) {
		id=idIn;
		name=nameIn;
		retail=Double.parseDouble(retailIn);
	}

}
